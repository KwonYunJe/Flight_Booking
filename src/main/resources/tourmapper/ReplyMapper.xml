<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="replyMap">
	<insert id="userCreate" parameterType="userVO">
		insert into USER values (#{id},#{pw},#{name},#{tel},#{gender},#{nickname})
	</insert>
	<insert id="bbsCreate" parameterType="bbsVO">
		insert into BBS values (#{title},#{contents},#{area},#{time})
	</insert>
	<insert id="bbsDelete" parameterType="bbsVO">
		delete from BBS  where id = #{bbsId}
	</insert>

</mapper>